 flex;\r\n  flex-direction: column;\r\n  justify-content: flex-end;\r\n  align-items: center;\r\n  gap: 6rem;\r\n  height: 74%;\r\n\r\n  max-width: 37.5rem;\r\n  margin: 0 auto;\r\n}\r\n\r\n.hero-section h1 {\r\n  text-align: center;\r\n  font-size: 4rem;\r\n  color: $white;\r\n  text-transform: uppercase;\r\n  letter-spacing: 6px;\r\n}\r\n\r\n.hero-arrow {\r\n  width: 3.5rem;\r\n  height: 11rem;\r\n}\r\n\r\n@include tablet-laptop-sm-layout() {\r\n  .hero-wrapper {\r\n    height: 64%;\r\n    gap: 4rem;\r\n  }\r\n\r\n  .hero-section h1 {\r\n    font-size: 6.6rem;\r\n  }\r\n}\r\n\r\n@include desktop-layout() {\r\n  .hero-wrapper {\r\n    max-width: none;\r\n    height: 64%;\r\n  }\r\n\r\n  .hero-section h1 {\r\n    font-size: 10rem;\r\n  }\r\n}\r\n","scss\\abstracts\\_mixins.scss":"@mixin tablet-laptop-sm-layout() {\r\n  @media only screen and (min-width: 600px) and (max-width: 1300px) {\r\n    @content;\r\n  }\r\n}\r\n\r\n@mixin desktop-layout() {\r\n  @media only screen and (min-width: 1300px) {\r\n    @content;\r\n  }\r\n}\r\n","scss\\layout\\_product-section.scss":"@use \"../abstracts\" as *;\r\n\r\n.product-section .product {\r\n  display: grid;\r\n}\r\n\r\n.product-img {\r\n  order: 1;\r\n}\r\n\r\n.product-content {\r\n  order: 2;\r\n\r\n  max-width: 375px;\r\n  margin: 0 auto;\r\n\r\n  text-align: center;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 2.4rem;\r\n\r\n  padding: 5.6rem 4rem;\r\n}\r\n\r\n.product-content h2 {\r\n  color: $very-dark-desaturated-blue;\r\n  font-size: 3.2rem;\r\n}\r\n\r\n.product-wrapped {\r\n  display: grid;\r\n  grid-template-rows: 540px 620px;\r\n}\r\n\r\n.product-wrapped-container {\r\n  text-align: center;\r\n\r\n  position: relative;\r\n  padding: 0 2rem 6rem;\r\n\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: flex-end;\r\n}\r\n\r\n.product-wrapped-img {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: -10;\r\n}\r\n\r\n.product-wrapped-con